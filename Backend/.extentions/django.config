    WSGIPath:Server.wsgi:application

  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: your_project_name.settings
    PYTHONPATH: /var/app/current
    ALLOWED_HOSTS: .elasticbeanstalk.com
